{
    "name": "@gx/gi",
    "version": "1.0.0",
    "description": "Gtk platform types generated by ts-for-gir.",
    "license": "LGPL-3.0+",
    "keywords": [],
    "author": {
        "name": "Bodil Stokke",
        "email": "bodil@bodil.org"
    },
    "type": "module",
    "main": "./index.ts",
    "typedoc": {
        "entryPoint": "./index.ts"
    },
    "scripts": {
        "clean": "rm -rf Gjs index.ts",
        "build:types": "pnpm ts-for-gir generate --configName=ts-for-girrc.json",
        "build:types:missing": "node ./buildTypes.js",
        "build:index": "node ./buildIndex.js",
        "build": "run-s build:types:missing build:index",
        "check": "tsc -p tsconfig.json --noEmit"
    },
    "devDependencies": {
        "@ts-for-gir/cli": "3.0.0-beta.2",
        "@types/node": "^18.7.13"
    }
}
